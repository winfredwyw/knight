        "style-loader": "^0.21.0",
        "url-loader": "^1.0.1",
        "webpack": "^4.11.0",
        "webpack-api-mocker": "^1.5.2",
        "webpack-dev-server": "^3.1.4",
        "webpack-manifest-plugin": "^2.0.3"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-console": {},
            "cordova-plugin-device": {},
            "cordova-plugin-statusbar": {},
