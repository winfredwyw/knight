{
    "name": "helloworld",
    "displayName": "HelloCordova",
    "version": "1.0.0",
    "description": "A sample Apache Cordova application that responds to the deviceready event.",
    "main": "index.js",
    "scripts": {
        "start": "cross-env node scripts/npmscripts/start",
        "dev": "npm run start",
        "build": "cross-env node scripts/npmscripts/build",
        "cordova": "cordova platform add android && cordova platform add ios",
        "build:android": "cordova build android --aot --prod --release",
        "lint": "./node_modules/.bin/eslint src/**/*.js",
        "android": "cordova run android",
        "ios": "cordova run ios",
        "build:ios": "cordova build ios --aot --prod --release"
    },
    "author": "Apache Cordova Team",
    "license": "Apache-2.0",
    "dependencies": {
        "cordova-android": "^6.2.3",
        "cordova-ios": "^4.4.0",
